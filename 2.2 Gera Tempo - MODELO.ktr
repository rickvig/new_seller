if(datDia>=20 &amp;&amp; datMes>=3 &amp;&amp; datMes&lt;=5) {
	strEstacao = "OUTONO";
}
if(datDia>=21 &amp;&amp; datMes>=6 &amp;&amp; datMes&lt;=8) {
	strEstacao ="INVERNO";
}
if(datDia>=23 &amp;&amp; datMes>=9 &amp;&amp; datMes&lt;=11) {
	strEstacao = "PRIMAVERA";
}  
if(datDia>=21 &amp;&amp; datMes>=12) {
	strEstacao = "VERAO";
}
if(datMes>=1 &amp;&amp; datMes&lt;=2) {
	strEstacao = "VERAO";
